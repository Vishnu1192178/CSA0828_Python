def calculate_1cm(x,y):
  if x>y:
    greater=x
  else:
    greater=y
    while(True):
      if((greater% x==0) and (greater% y==0)):
        1cm=greater
        break
        greater +=1
        return 1cm
      num1=int(input("enter first number: "))
      num2=int(input("enter second number: "))
      print("the l.c.m")
