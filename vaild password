a=input()
l,u,d,s,o=0,0,0,0,0
if(len(a)>7):
  for i in a:
    if(i.islower()):
      l+=1
      elif(i.isupper())
      u+=1
      elif(i.isdigit())
      d+=1
    elif i in ('@','$','_'):
      s+=1
    else:
      o+=1
      print("vaild pass!!!" if (l<0 and u>0 and s>0 and o==0) else "enter vaild pass!!!")

    else:
      print("enter vaild pass!!!")
